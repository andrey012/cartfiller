{
  "name": "Cartfiller Bootstrap",
  "description": "Launch Cartfiller",
  "version": "1.0",
  "permissions": [
    "activeTab",
    "webNavigation",
    "*://*/*"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_title": "None"
  },
  "manifest_version": 2
}
